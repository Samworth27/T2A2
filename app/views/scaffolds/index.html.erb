<p style="color: green"><%= notice %></p>

<h1 class="text-secondary">Scaffolds</h1>

<div id="scaffolds">
  <% @scaffolds.each do |scaffold| %>
    <%= render scaffold %>
    <p>
      <%= link_to "Show this scaffold", scaffold %>
    </p>
  <% end %>
</div>

<%= link_to "New scaffold", new_scaffold_path %>
